  "bugs": {
    "url": "https://github.com/suisei-cn/holodailybot/issues"
  },
  "homepage": "https://github.com/suisei-cn/holodailybot#readme",
  "dependencies": {
    "express": "^4.17.1",
    "node-fetch": "^2.6.0",
    "seedrandom": "^3.0.5",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/express": "^4.17.6",
    "@types/mocha": "^7.0.2",
    "chai": "^4.2.0",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
