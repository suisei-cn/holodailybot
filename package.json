  "description": "A interesting telegram bot.",
  "main": "src/index.js",
  "scripts": {
    "start:dev": "nodemon dist/cli.js",
    "build:dev": "tsc --watch --preserveWatchOutput",
    "dev": "concurrently \"npm:build:dev\" \"npm:start:dev\"",
